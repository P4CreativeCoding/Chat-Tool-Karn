{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/socket.io",
      "dest": "http://dein-websocket-endpunkt/socket.io"
    },
    {
      "src": "/api/.*",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
